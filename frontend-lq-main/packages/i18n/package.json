{
  "name": "@lq/i18n",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./locales/*": "./src/locales/*/index.ts",
    "./locales/*/*": "./src/locales/*/*.ts",
    "./config": "./src/config.ts",
    "./types": "./src/types.ts",
    "./utils/*": "./src/utils/*.ts",
    "./composables/*": "./src/composables/*.ts"
  },
  "scripts": {
    "build": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "types:generate": "tsx scripts/generate-types.ts",
    "extract:hardcoded": "tsx scripts/extract-hardcoded.ts",
    "replace:hardcoded": "tsx scripts/replace-hardcoded.ts"
  },
  "dependencies": {
    "vue": "^3.5.0",
    "vue-i18n": "^10.0.0"
  },
  "devDependencies": {
    "@babel/parser": "^7.24.0",
    "@babel/traverse": "^7.24.0",
    "@types/babel__traverse": "^7.20.5",
    "@types/node": "^20.14.10",
    "glob": "^10.3.0",
    "tsx": "^4.7.0",
    "typescript": "^5.5.3"
  }
}
